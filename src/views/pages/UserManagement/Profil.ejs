<%- include("../../partials/header") %>
<%- include("../../partials/nav") %>
<!-- Main content -->
<div class="main-content row mt-4">
    <div class="col-1"></div>
    <div class="col card">
        <h1 class="text-center">Mon Profil</h1>
        <img src="<%= session.user.photo_url %>" class="rounded-circle img-profil my-5" alt="Responsive image">
        <h3 class="info-profil"><i class="fas fa-user"></i> <%= session.user.firstName + " " + session.user.lastName %></h3>
        <h3 class="info-profil"><i class="fas fa-phone-alt"></i> <%= session.user.tel_nb %></h3>
        <h3 class="info-profil"><i class="fas fa-envelope"></i> <%= session.user.mail %></h3>
        <h3 class="info-profil"><i class="fas fa-home"></i> <%= session.user.street_nb + " " + session.user.street + ", " + session.user.postal_code + " " + session.user.city %></h3>
        <h3 class="info-profil"><i class="fas fa-birthday-cake"></i> <%= session.user.date_birth %></h3>
        <h3 class="info-profil"><i class="fas fa-concierge-bell"></i> <%= session.user.id_service %></h3>
    </div>
    <div class="col-1"></div>
</div>
<%- include("../../partials/footer") %>